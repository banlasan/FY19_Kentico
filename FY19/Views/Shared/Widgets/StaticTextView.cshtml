@using Kentico.PageBuilder.Web.Mvc
@using FY19.Models.Widgets
@using FY19.Models.InlineEditors
@model ComponentViewModel<StaticTextModelProperties>


@{
    string htmlText = string.Empty;

    if (Model.Properties.Text != null)
    { htmlText = Model.Properties.Text; }
    else { htmlText = Model.Properties.Text; }
}

@Html.Raw(HttpUtility.HtmlDecode(htmlText))